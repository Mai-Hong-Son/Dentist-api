{
  "presets": [
    "es2015",
    "flow"
  ],
  "retainLines": true,
  "plugins": [
    ["transform-object-rest-spread"],
    "add-module-exports",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "app": "./"
        }
      }
    ]
  ]
}