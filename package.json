{
  "name": "api-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./dist,./node_modules,./.babelrc,./package.json,./npm-debug.log,.yarn-error.log --copy-files",
    "start": "yarn build && node index",
    "dev": "node_modules/.bin/cross-env NODE_ENV=development babel-node index",
    "seq": "babel ./node_modules/.bin/sequelize",
    "migrate": "babel-node ./node_modules/.bin/sequelize db:migrate",
    "seed:all": "babel-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:all@win": "babel-node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "test": "mocha --require babel-core/register ./test"
  },
  "dependencies": {
    "acl": "^0.4.11",
    "acl-sequelize": "^0.2.1",
    "bcrypt": "^2.0.1",
    "connect-roles": "^3.1.2",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "express-fileupload": "^0.4.0",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.2.0",
    "faker": "^4.1.0",
    "http-errors": "~1.6.2",
    "i18next": "^11.3.3",
    "i18next-express-middleware": "^1.2.0",
    "i18next-node-fs-backend": "^1.1.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-google-oauth": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "sequelize": "^4.38.0",
    "sinon": "^6.1.3",
    "sharp": "^0.20.5",
    "uuid": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "flow-bin": "^0.75.0",
    "mocha": "^5.2.0",
    "mysql": "^2.15.0",
    "rimraf": "^2.6.2",
    "sequelize-auto": "^0.4.29",
    "sequelize-cli": "^4.0.0"
  }
}
